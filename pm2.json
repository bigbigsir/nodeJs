{
  "apps": [
    {
      "cwd": "./",
      "name": "port_80",
      "script": "port_80.js",
      "merge_logs": true,
      "env": {
        "NODE_ENV": "development",
        "NETWORK_ENV": "inner"
      },
      "out_file": "./logs/port_80/out.log",
      "error_file": "./logs/port_80/error.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss"
    },
    {
      "cwd": "./",
      "name": "port_3000",
      "script": "port_3000.js",
      "merge_logs": true,
      "env": {
        "NODE_ENV": "development",
        "NETWORK_ENV": "inner"
      },
      "out_file": "./logs/port_3000/out.log",
      "error_file": "./logs/port_3000/error.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss"
    }
  ]
}
